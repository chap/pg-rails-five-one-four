{
  "name": "pg-rails-five-one-four",
  "scripts": {
    "postdeploy": "rake db:schema:load"
  },
  "env": {
    "LANG": {
      "required": true
    },
    "RACK_ENV": {
      "required": true
    },
    "RAILS_ENV": {
      "required": true
    },
    "RAILS_LOG_TO_STDOUT": {
      "required": true
    },
    "RAILS_SERVE_STATIC_FILES": {
      "required": true
    },
    "SECRET_KEY_BASE": {
      "required": true
    }
  },
  "formation": {
  },
  "addons": [
       {
      "plan": "heroku-postgresql",
      "options": {
        "version": "10"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/ruby"
    }
  ]
}

